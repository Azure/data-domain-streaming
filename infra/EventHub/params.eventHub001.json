{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "eventHubName": {
      "value": "<my-data-landing-zone-name>-eventhub001"
    },
    "eventHubSku": {
      "value": "Standard"
    },
    "eventHubMinThroughputUnits": {
      "value": 1
    },
    "eventHubMaxThroughputUnits": {
      "value": 2
    },
    "keyVaultId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-rg/providers/Microsoft.KeyVault/vaults/<my-data-landing-zone-name>-keyvault001"
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    },
    "privateDnsZoneId": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.servicebus.windows.net"
    }
  }
}
