{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "iotHubName": {
      "value": "<my-data-landing-zone-name>-iothub001"
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    },
    "privateDnsZoneIdIotHub": {
      "value": "/subscriptions/<my-management-subscription-id>/resourceGroups/<my-global-dns-resource-group-name>/providers/Microsoft.Network/privateDnsZones/privatelink.azure-devices.net"
    },
    "privateDnsZoneIdEventHub": {
      "value": "/subscriptions/<my-management-subscription-id>/resourceGroups/<my-global-dns-resource-group-name>/providers/Microsoft.Network/privateDnsZones/privatelink.servicebus.windows.net"
    }
  }
}
