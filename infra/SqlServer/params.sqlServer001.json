{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "sqlServerName": {
      "value": "<my-data-landing-zone-name>-sqlserver001"
    },
    "administratorLogin": {
      "value": "ServerMainUser"
    },
    "administratorLoginPassword": {
      "value": "<your-secure-password>"
    },
    "sqlAdminGroupName": {
      "value": ""
    },
    "sqlAdminGroupObjectID": {
      "value": ""
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    },
    "privateDnsZoneId": {
      "value": "<my-global-dns-resource-group-resource-id>/providers/Microsoft.Network/privateDnsZones/privatelink.database.windows.net"
    }
  }
}
