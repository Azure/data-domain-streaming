{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "streamAnalyticsClusterName": {
      "value": "<my-data-landing-zone-name>-streamanalyticscluster001"
    },
    "streamAnalyticsClusterCapacity": {
      "value": 36
    },
    "storageAccountId": {
      "value": ""
    },
    "eventHubId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-rg/providers/Microsoft.EventHub/namespaces/<my-data-landing-zone-name>-eventhub001"
    },
    "sqlServerId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-rg/providers/Microsoft.Sql/servers/<my-data-landing-zone-name>-sqlserver001"
    },
    "cosmosDbId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-rg/providers/Microsoft.DocumentDB/databaseAccounts/<my-data-landing-zone-name>-cosmos001"
    }
  }
}
